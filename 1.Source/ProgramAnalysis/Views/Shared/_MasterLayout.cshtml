<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap -->
    <link href="@Url.Content("~/Content/themes/vendors/bootstrap/dist/css/bootstrap.min.css")" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="@Url.Content("~/Content/themes/vendors/font-awesome/css/font-awesome.min.css")" rel="stylesheet">
    <!-- NProgress -->
    <link href="@Url.Content("~/Content/themes/vendors/nprogress/nprogress.css")" rel="stylesheet">
    <!-- iCheck -->
    <link href="@Url.Content("~/Content/themes/vendors/iCheck/skins/flat/green.css")" rel="stylesheet">

    <!-- bootstrap-progressbar -->
    <link href="@Url.Content("~/Content/themes/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css")" rel="stylesheet">
    <!-- JQVMap -->
    <link href="@Url.Content("~/Content/themes/vendors/jqvmap/dist/jqvmap.min.css")" rel="stylesheet" />
    <!-- bootstrap-daterangepicker -->
    <link href="@Url.Content("~/Content/themes/vendors/bootstrap-daterangepicker/daterangepicker.css")" rel="stylesheet">
    <!-- Custom Theme Style -->
    <link href="@Url.Content("~/Content/themes/build/css/custom.min.css")" rel="stylesheet">
</head>
<body class="nav-md">
    @RenderBody()

    <!-- jQuery -->
    <script src="@Url.Content("~/Content/themes/vendors/jquery/dist/jquery.min.js")"></script>
    <!-- Bootstrap -->
    <script src="@Url.Content("~/Content/themes/vendors/bootstrap/dist/js/bootstrap.min.js")"></script>
    <!-- FastClick -->
    <script src="@Url.Content("~/Content/themes/vendors/fastclick/lib/fastclick.js")"></script>
    <!-- NProgress -->
    <script src="@Url.Content("~/Content/themes/vendors/nprogress/nprogress.js")"></script>
    <!-- Chart.js -->
    <script src="@Url.Content("~/Content/themes/vendors/Chart.js/dist/Chart.min.js")"></script>
    <!-- gauge.js -->
    <script src="@Url.Content("~/Content/themes/vendors/gauge.js/dist/gauge.min.js")"></script>
    <!-- bootstrap-progressbar -->
    <script src="@Url.Content("~/Content/themes/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js")"></script>
    <!-- iCheck -->
    <script src="@Url.Content("~/Content/themes/vendors/iCheck/icheck.min.js")"></script>
    <!-- Skycons -->
    <script src="@Url.Content("~/Content/themes/vendors/skycons/skycons.js")"></script>
    <!-- Flot -->
    <script src="@Url.Content("~/Content/themes/vendors/Flot/jquery.flot.js")"></script>
    <script src="@Url.Content("~/Content/themes/vendors/Flot/jquery.flot.pie.js")"></script>
    <script src="@Url.Content("~/Content/themes/vendors/Flot/jquery.flot.time.js")"></script>
    <script src="@Url.Content("~/Content/themes/vendors/Flot/jquery.flot.stack.js")"></script>
    <script src="@Url.Content("~/Content/themes/vendors/Flot/jquery.flot.resize.js")"></script>
    <!-- Flot plugins -->
    <script src="@Url.Content("~/Content/themes/vendors/flot.orderbars/js/jquery.flot.orderBars.js")"></script>
    <script src="@Url.Content("~/Content/themes/vendors/flot-spline/js/jquery.flot.spline.min.js")"></script>
    <script src="@Url.Content("~/Content/themes/vendors/flot.curvedlines/curvedLines.js")"></script>
    <!-- DateJS -->
    <script src="@Url.Content("~/Content/themes/vendors/DateJS/build/date.js")"></script>
    <!-- JQVMap -->
    <script src="@Url.Content("~/Content/themes/vendors/jqvmap/dist/jquery.vmap.js")"></script>
    <script src="@Url.Content("~/Content/themes/vendors/jqvmap/dist/maps/jquery.vmap.world.js")"></script>
    <script src="@Url.Content("~/Content/themes/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js")"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="@Url.Content("~/Content/themes/vendors/moment/min/moment.min.js")"></script>
    <script src="@Url.Content("~/Content/themes/vendors/bootstrap-daterangepicker/daterangepicker.js")"></script>
    <!-- Custom Theme Scripts -->
    <script src="@Url.Content("~/Content/themes/build/js/custom.min.js")"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
